-- h√†m tp --
local function findPlayerByDisplayName(name)
	name = string.lower(name)
	for _, plr in pairs(game.Players:GetPlayers()) do
		if string.lower(plr.DisplayName) == name or string.lower(plr.Name) == name then
			return plr
		end
	end
	return nil
end

